					        			                  <span class="data"></span>
					        			                  <span class="lbl">{{ days_name }}</span>
					        			                </div>
					        			                <div class='countdown-hours counter'>
					        			                  <span class="data"></span>
					        			                  <span class="lbl">{{ hours_name }}</span>
					        			                </div>
					        			                <div class='countdown-minutes counter'>
					        			                  <span class="data"></span>
					        			                  <span class="lbl">{{ min_name }}</span>
					        			                </div>
					        			                <div class='countdown-seconds counter'>
					        			                  <span class="data"></span>
					        			                  <span class="lbl">{{ sec_name }}</span>
					        			                </div>
					        			            </div>
						      	                  {% endif %} 
						                        </div>                  
						                        <div class="caption">   
						                          <h4><a href="{{ product.href }}">{{ product.name }}</a></h4>
						                          <p class="description">{{ product.description }}</p>
						                          {% if product.price %}
						                          <p class="price">
						                            {% if not product.special %}
						                            {{ product.price }}
						                            {% else %}
						                            <span class="price-old">{{ product.price }}</span> <span class="price-new">{{ product.special }}</span> 
						                            {% endif %}
						                            {% if product.tax %}
						                            <span class="price-tax">{{ text_tax }} {{ product.tax }}</span>
						                            {% endif %}
						                          </p>
						                          {% endif %}
						                          	<div class="btn-cart">
											            <button type="submit" formaction="{{ add_to_cart }}" data-bs-toggle="tooltip" title="{{ button_cart }}" {% if product.stock_status %} class="sold-out" disabled{% endif %}>
											               {% if product.stock_status %} <span class="lblcart">{{ button_soldout }}</span> {% else %} <span class="lblcart">{{ button_cart }}</span> {% endif %}
											            </button>
											        </div>
						                        </div>
						                    </div>
						                </div> 
						                <input type="hidden" name="product_id" value="{{ product.product_id }}"/>
    									<input type="hidden" name="quantity" value="{{ product.minimum }}"/>
						        </form>         
					        {% if product_row != 1 %}
					        {% if counter % product_row == 0 or counter == lastproduct %}
					        </div>   
				            {% endif %}
				            {% endif %}	
				            {% set counter = counter+1 %}
				            {% endfor %}        
				    	</div>        
					</div>
				</div>
			</div>
			{% endfor %}
	</div>
  <script type="text/javascript">
    $('#{{ ishi_randomnumer }} .owl-carousel').owlCarousel({
        loop:false,
        nav:true,  
        dots:false, 
        rewind: true,
        rtl: $('html').attr('dir') == 'rtl'? true : false,
        navText: ["<i class='fa fa-angle-left'></i>","<i class='fa fa-angle-right'></i>"], 
        responsive:{
            0:{
                items:{{ mobile_column }}
            },
            576:{
                items:{{ tablet_column }}
            },
            768:{
                items:{{ laptop_column }}
            },
            992:{
                items:{{ laptop_column }}
            },
            1200:{
                items:{{ desktop_column }}
            }
        }
    });   
  </script>
 
</section>