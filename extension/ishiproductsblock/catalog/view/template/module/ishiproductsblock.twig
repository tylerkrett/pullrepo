<section id="{{ ishi_randomnumer }}" class="ishiproductsblock container">
    <div class="section-header">  
      <h3 class="home-title">{{ heading }}</h3>
	  <p class="desc"> {{ desc }}</p>
    </div>
    <ul class="ishiproductstab nav nav-tabs clearfix">
	    {% if featured %}
	      <li class="nav-item">
	        <a class="nav-link active" href="#featured-products-block{{ tab_randomnumer }}" data-bs-toggle="tab">{{ heading_featured }}</a>
	      </li>
	    {% endif %}
	    {% if bestseller %}
	      <li class="nav-item">
	        <a class="nav-link" href="#bestseller-products-block{{ tab_randomnumer }}" data-bs-toggle="tab">{{ heading_bestseller }}</a>
	      </li>
	    {% endif %}
	    {% if new %}
	      <li class="nav-item">
	        <a class="nav-link" href="#new-products-block{{ tab_randomnumer }}" data-bs-toggle="tab">{{ heading_new }}</a>
	      </li>
	    {% endif %}
	    {% if special %}
	      <li class="nav-item">
	        <a class="nav-link" href="#special-products-block{{ tab_randomnumer }}" data-bs-toggle="tab">{{ heading_special }}</a>
	      </li>
	    {% endif %}
		{% if category %}
			{% for category in product_categories %}
			  <li class="nav-item">
				<a class="nav-link " href="#category-block{{ category.category_id }}-{{ tab_randomnumer }}" data-bs-toggle="tab">{{ category.name }}</a>
			  </li>
			{% endfor %}
		{% endif %}
	</ul>
	<div class="tab-content">
		    <div class="tab-pane active" id="featured-products-block{{ tab_randomnumer }}">
		     <div class="block_content">  
		     	<div class="row">      
			        <div class="owl-carousel"> 
			          {% set counter = 1 %}
			          {% set lastproduct = featured|length %} 
			          {% for product in featured %}
			            {% if product_row != 1 %}
			              {% if counter % product_row == 1 %}
			                <div class="multilevel-item">
			                  {% endif %}
			                  {% endif %}
			                <form method="post" data-oc-toggle="ajax" data-oc-load="{{ cart }}" data-oc-target="#header-cart">
				                <div class="item product-container" data-countdowntime="{{ product.date_end }}"> 
				                    <div class="product-thumb"> 
				                      <div class="image">
				                        <a href="{{ product.href }}">
				                          <img src="{{ product.thumb }}" alt="{{ product.name }}" title="{{ product.name }}" class="img-responsive" />
				                            {% if product.extra != '' and hover_image == 1 %}
				                              <img class="product-img-extra img-responsive" alt="{{ product.name }}" title="{{ product.name }}" src= "{{ product.extra }}"/>
				                            {% endif %}
				                        </a>
				                        {% if product.stock_status %} <span class="outstock-overlay">{{ product.stock_status }}</span> {% endif %}
				                        <div class="button-group">  
				                            <div class="btn-quickview"> 
				                              <div class="quickview-button button" data-bs-toggle="tooltip" title=" {{ quick_view }}"> 
				                                <a class="quickbox" href="{{ product.quick }}">
