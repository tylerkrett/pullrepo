{{ header }}
<div class="breadcrumb-container">
  <h1 class="page-title">{{ heading_title }}</h1>
  <ul class="breadcrumb">
      {% for breadcrumb in breadcrumbs %}
      <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
      {% endfor %}
  </ul>
</div>
<div id="common-success" class="container">
  <div class="row">
    <div id="content" class="col">{{ content_top }}{{ content_center }}
      <h3 class="mb-3 success-title"><i class="fas fa-check-circle text-success"></i>{{ heading_title }}</h3>
      {{ text_message }}
      <div class="d-inline-block pt-2 pd-2 w-100">
        <div class="float-start"><a href="{{ continue }}" class="btn btn-primary">{{ button_continue }}</a></div>
      </div>
      {{ content_bottom }}</div>
    {{ column_right }}</div>
</div>
{{ footer }}
  